<div class="container">
  <h1>Create a new booking</h1>
  <%= simple_form_for [@watch, Booking.new] do |f| %>
    <%= f.input :booking_start_date, as: :date, html5: true %>
    <%= f.input :booking_end_date, as: :date, html5: true  %>
    <%= f.submit "add a booking", class: "btn btn-success" %>
  <% end %>
</div>
